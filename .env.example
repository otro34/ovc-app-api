# Server Configuration
PORT=3001
NODE_ENV=development

# Data Storage
DATA_DIR=./data

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRATION=8h

# Logging
LOG_LEVEL=info

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_FREQUENCY=daily

# Security
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
